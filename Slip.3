import java.util.Scanner;

class MyException extends Exception {
    public MyException(String args) {
        super(args);
    }
}

public class rough {
    
    class Patient {
        String pname;
        int age;
        double olevel, hrct;
        
        void getdata() {
            Scanner sc = new Scanner(System.in);
            System.out.println("Enter Patient Details: ");
            System.out.print("Name: ");
            pname = sc.next();
            System.out.print("Age: ");
            age = sc.nextInt();
            System.out.print("Oxygen Level: ");
            olevel = sc.nextDouble();
            System.out.print("HRCT Report: ");
            hrct = sc.nextDouble();
        }
        
        void check() {
            try {
                if (olevel < 95 && hrct > 10) {
                    throw new MyException("Patient is Covid Positive(+) and Need to Hospitalized");
                } else {
                    System.out.println("\nPatient Information:");
                    System.out.println("Name: " + pname);
                    System.out.println("Age: " + age);
                    System.out.println("Oxygen Level: " + olevel + "%");
                    System.out.println("HRCT Report: " + hrct);
                    System.out.println("Patient is COVID Negative(-)");
                }
            } catch (MyException me) {
                System.out.println("\n" + me);
            }
        }
    }

    public static void main(String[] args) {
        rough covid = new rough();
        Patient patient = covid.new Patient(); 
        
        patient.getdata();
        patient.check();
    }
}
