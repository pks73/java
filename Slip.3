import java.util.Scanner;

class MyException extends Exception{
	public MyException(String args) {
		super(args);
	}
}

class cov {
		String pname;
		int age;
		double olevel,hrct;
		
		void getdata()
		{
			Scanner sc=new Scanner(System.in);
			System.out.println("Enter Patient Details Name, Age, Oxy Level, HRCT: ");
			pname=sc.next();
			age=sc.nextInt();
			olevel=sc.nextDouble();
			hrct=sc.nextDouble();
		}
		
		void check() 
		{
			try 
			{
				if(olevel <95 && hrct >10) 
				{
					throw new MyException("PatienrIsCovidPositive");
				}
				else 
				{
					System.out.println("Name: "+pname+"\nAge: "+age+"\nOxy Level: "+olevel+"\nHRCT: "+hrct);
				}
			}
			catch(MyException me) 
			{
				System.out.println(me);
			}
		}
}
class Main1{	
	public static void main(String[] args) {

		cov c=new cov();
		c.getdata();
		c.check();

	}
}

